<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="local/gradereport_submit/db" VERSION="2023100600" COMMENT="Grade report submission plugin">
  <TABLES>
  <TABLE NAME="gradereport_submissions" COMMENT="Stores submitted gradebook reports">
    <FIELDS>
    <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
    <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true"/>
    <FIELD NAME="approved" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
    <FIELD NAME="report_data" TYPE="string" LENGTH="1000" NOTNULL="true" DEFAULT=""/>
    <FIELD NAME="submiited_by" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
    <FIELD NAME="submiited_data" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
    </FIELDS>
    <KEYS>
      <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
    </KEYS>
  </TABLE>
    </TABLES>
</XMLDB>

